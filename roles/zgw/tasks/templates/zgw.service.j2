# {{ ansible_managed }}

[Unit]
Description=ZGW services
After=docker.service

[Service]
Type=oneshot
RemainAfterExit=yes
StandardError=null
StandardOutput=null
WorkingDirectory=/opt/zgw
ExecStart=/usr/local/bin/docker-compose up -d
ExecStop=/usr/local/bin/docker-compose down -t 1

[Install]
WantedBy=multi-user.target
