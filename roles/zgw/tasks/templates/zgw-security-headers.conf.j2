# {{ ansible_managed }}

add_header X-Content-Type-Options "nosniff" always;
add_header X-Frame-Options "SAMEORIGIN" always;
add_header X-XSS-Protection "1; mode=block" always;
add_header Referrer-Policy "no-referrer" always;
add_header Content-Security-Policy "default-src 'self'; script-src 'self' 'unsafe-inline' https://code.jquery.com/ https://stackpath.bootstrapcdn.com/ https://cdnjs.cloudflare.com/ https://maps.googleapis.com https://maps.gstatic.com; style-src 'self' 'unsafe-inline' https://stackpath.bootstrapcdn.com/ https://use.fontawesome.com/ https://fonts.googleapis.com/; font-src 'self' https://use.fontawesome.com/ https://fonts.googleapis.com/ https://fonts.gstatic.com/; img-src 'self' data: https://vng.nl/ https://maps.gstatic.com/ https://maps.googleapis.com/" always;
add_header Feature-Policy "accelerometer 'none'; camera 'none'; geolocation 'none'; gyroscope 'none'; magnetometer 'none'; microphone 'none'; payment 'none'; usb 'none'" always;
